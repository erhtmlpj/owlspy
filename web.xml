<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Servlets.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registra</servlet-name>
        <servlet-class>Servlets.Registra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Servlets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SalvarPC</servlet-name>
        <servlet-class>Servlets.SalvarPC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Connectar</servlet-name>
        <servlet-class>Servlets.Connectar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Msenha</servlet-name>
        <servlet-class>Servlets.Msenha</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dconta</servlet-name>
        <servlet-class>Servlets.Dconta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>conSalvo</servlet-name>
        <servlet-class>Servlets.conSalvo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registra</servlet-name>
        <url-pattern>/Registra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalvarPC</servlet-name>
        <url-pattern>/SalvarPC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Connectar</servlet-name>
        <url-pattern>/Connectar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Msenha</servlet-name>
        <url-pattern>/Msenha</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dconta</servlet-name>
        <url-pattern>/Dconta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>conSalvo</servlet-name>
        <url-pattern>/conSalvo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
